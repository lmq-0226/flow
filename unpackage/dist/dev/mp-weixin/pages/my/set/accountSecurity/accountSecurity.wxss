.content .auth {
  border-top: solid 10rpx #F6F5FA;
  padding: 0 36rpx;
}
.content .auth view {
  padding: 37rpx 40rpx 37rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: solid 1px #F6F5FA;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 44rpx;
}
.content .auth view text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
}
.content .gap {
  padding: 25rpx 36rpx;
  background: #F6F4FA;
}
.content .gap text {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #686879;
}
.content .wx {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 28rpx 37rpx;
  border-bottom: solid 10rpx #F6F5FA;
}
.content .wx view {
  display: flex;
  align-items: center;
}
.content .wx view image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 27rpx;
}
.content .wx view text {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
}
.content .wx > text {
  padding: 8rpx 22rpx;
  border: 2px solid #9393A7;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
}
.content .cancel {
  padding: 0 36rpx;
}
.content .cancel text {
  padding: 37rpx 0;
  display: block;
  border-bottom: solid 1px #F6F5FA;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 44rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
}


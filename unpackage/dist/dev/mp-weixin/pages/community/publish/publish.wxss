.content {
  padding-bottom: 100rpx;
}
.content .pub {
  padding: 36rpx;
}
.content .pub .pubDetail {
  padding-bottom: 30rpx;
  border-bottom: solid 1px  #F7F7FB;
}
.content .pub .pubDetail .imgs {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.content .pub .pubDetail .imgs view {
  position: relative;
  margin-right: 17rpx;
}
.content .pub .pubDetail .imgs view > :nth-child(1) {
  height: 164rpx;
}
.content .pub .pubDetail .imgs view > :nth-child(2) {
  position: absolute;
  top: 0;
  right: 0;
  width: 35rpx;
  height: 35rpx;
}
.content .pub .pubDetail .imgs view text {
  position: absolute;
  padding: 8rpx 14rpx;
  background: #F55454;
  border-radius: 6rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  top: 0;
  left: 0;
}
.content .pub .pubDetail .imgs > image {
  width: 164rpx;
  height: 164rpx;
}
.content .pub .pubDetail .tags {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items: center;
}
.content .pub .pubDetail .tags > :not(:last-child) {
  margin-right: 15rpx;
}
.content .pub .pubDetail .tags .area {
  padding: 8rpx 16rpx;
  background: #F6F5FA;
  border-radius: 6rpx;
  margin-top: 20rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
}
.content .pub .pubDetail .tags .area image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 4rpx;
}
.content .pub .pubDetail .tags .area text {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #1D1D20;
}
.content .bottom {
  width: 100%;
  height: 98rpx;
  border-top: solid 1px #f2f2f2;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
  background: #fff;
}
.content .bottom view {
  text-align: center;
}
.content .bottom view image {
  width: 44rpx;
  height: 44rpx;
}
.content .bottom view text {
  display: block;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #000000;
}
.content .bottom > text {
  display: block;
  width: 557rpx;
  height: 74rpx;
  text-align: center;
  line-height: 74rpx;
  background: #F55454;
  border-radius: 6rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
}
.content .popups .title {
  padding: 20rpx 29rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .popups .title image {
  width: 44rpx;
  height: 44rpx;
}
.content .popups .title text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
}
.content .popups .tabs {
  border-bottom: solid 1px #F2F2F2;
}
.content .popups .search {
  padding: 20rpx 36rpx;
}
.content .popups .search .u-input {
  border-radius: 10rpx;
  background: url(data:image/png;base64,) no-repeat 20rpx center;
  background-size: 44rpx;
  background-color: #F6F5FA;
  padding-left: 75rpx !important;
  padding-right: 20rpx !important;
}
.content .popups .scroll-Y {
  width: 100%;
  height: 650rpx;
  background: #F6F5FA;
}
.content .popups .scroll-Y .goods .demo-warter {
  border-radius: 8px;
  margin: 5px;
  background-color: #ffffff;
  padding: 8px;
  position: relative;
}
.content .popups .scroll-Y .goods .demo-warter .demo-title {
  margin-top: 14rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
  overflow: hidden;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.content .popups .scroll-Y .goods .demo-warter .num {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.content .popups .scroll-Y .goods .demo-warter .num > :nth-child(1) {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FF4243;
}
.content .popups .scroll-Y .goods .demo-warter .num > :nth-child(2) {
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #9094A6;
}

